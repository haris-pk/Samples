pragma solidity ^0.8.7;

contract check{

   constructor(){
       uint number=10;
   }

    function addition(uint num2, uint num3) public pure returns(uint){
        return num2 + num3;
    }

    function multiplication(uint numberOne, numberTwo) public view returns(uint){
        return numberOne * numberTwo * number;
    }

    function modify() public retu)                           {
        return number; 
    }

}